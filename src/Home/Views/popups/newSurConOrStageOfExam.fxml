<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="168.0" prefWidth="358.0" xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="Home.Controllers.popup.newSurConOrStageOfExam">
   <children>
      <TextField fx:id="newSurfConditinoTXT" editable="false" layoutX="179.0" layoutY="40.0" prefHeight="26.0"
                 prefWidth="134.0" promptText="Surface Condition"/>
      <TextField fx:id="newStageOfExaminationTXT" editable="false" layoutX="180.0" layoutY="71.0" prefHeight="26.0"
                 prefWidth="134.0" promptText="Stage of Examination"/>
      <CheckBox fx:id="NewSurfaceConCheck" layoutX="126.0" layoutY="44.0" mnemonicParsing="false"
                onMouseClicked="#setNewSurfCondition"/>
      <CheckBox fx:id="NewStageOfExaminationCheck" layoutX="126.0" layoutY="76.0" mnemonicParsing="false"
                onMouseClicked="#setStageOfExam"/>
      <Button fx:id="saveButton" layoutX="45.0" layoutY="59.0" mnemonicParsing="false" onAction="#save" text="Save"/>
      <Button fx:id="cancelButton" layoutX="153.0" layoutY="120.0" mnemonicParsing="false" onAction="#cancel"
              text="Cancel"/>
   </children>
</AnchorPane>
